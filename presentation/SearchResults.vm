<html>
    <head>
    		#include("Meta.vm")
            <title>Martus Amplifier Search Results</title>
    </head>
    <body>
		#include ("ResultHeader.vm")
		
		<br/><br/><br/>
        
        <table width="800" border="0" cellpadding="0" cellspacing="0">
			#foreach ($thisBulletin in $foundBulletins)
	            <tr>
	            	<td valign='top'>
	            		$velocityCount. 
	            	</td>
	                <td>
	                	<strong>Title<strong>: <a href="FoundBulletin?index=$velocityCount">$thisBulletin.fields.title</a><br/>
		                <strong>Author</strong>: $thisBulletin.fields.author<br/>
		                <strong>Event Date</strong>: $thisBulletin.fields.eventdate<br/>
		            </td>
	            </tr>
			
			#end
        </table>
        
		<br/><br/><br/>
		#parse ("ResultFooter.vm")
    </body>
</html>
