<html>
    <head>
   		#parse("Meta.vm")
    	<title>Martus Amplifier Search Query</title>
    	<!--Account Public Code = $accountPublicCode -->
    	<!--Bulletin Local Id = $bulletinLocalId -->
    </head>
    
    <body>
    	#set ($pageTitle="Searched Martus for: $searchedFor")
    	#set ($pageInfo="Bulletin $currentBulletin of $totalBulletins")
		#set ($showSearchForm = true)
		#parse ("ResultHeader.vm")
		<p><font size='-1'><a href='PrintFoundBulletin?index=$currentBulletin&searchedFor=$searchedFor'>Printer Friendly Version</a></font></p>
		#parse("PreviousNextBulletin.vm")
		<br/>		
		#parse ("DisplayBulletinInfo.vm")
		<br/>

		#parse("PreviousNextBulletin.vm")
		#set ($showSearchForm = true)
		#parse ("ResultFooter.vm")
    </body>
</html>
		