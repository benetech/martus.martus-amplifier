<html>
    <head>
   		#include("Meta.vm")
    	<title>Martus Amplifier Search Query</title>
    </head>
    
    <body>
		#include ("ResultHeader.vm")
		
		<p><font size='-1'><a href='some url'>Printer Friendly Version</a></font></p>
		<br/>
		#parse("PreviousNextBulletin.vm")
		<br/>		
		<table>
			<tr>
				<td><font size="+1">Title:</font></td>
				<td><font size="+1"><strong>$bulletin.fields.title</strong></font></td>
			
			</tr>
			<tr>
				#if($bulletin.fields.location != "")
					<td>Location:</td>
					<td>$bulletin.fields.location</td>
				#end
				<td></td>
			</tr>
			<tr>
				<td>Event Date:</td>
				<td>$bulletin.fields.eventdate</td>
				<td></td>
			</tr>
			<tr>
				#if($bulletin.fields.author != "")
					<td>Author:</td>
					<td>$bulletin.fields.author</td>
				#end
				<td></td>
			</tr>
			<tr>
				<td>Date Submitted:</td>
				<td>$bulletin.fields.entrydate</td>
				<td></td>
			</tr>
			<tr>
				#if($bulletin.fields.summary != "")
					<td>Summary:</td>
					<td>$bulletin.fields.summary</td>
				#end
				<td></td>
			</tr>
			<tr>
				#if($bulletin.fields.publicinfo != "")
					<td>Details:</td>
					<td>$bulletin.fields.publicinfo</td>
				#end
				<td></td>
			</tr>
			<tr>
				#if($bulletin.fields.keywords != "")
					<td>Keywords:</td>
					<td>$bulletin.fields.keywords</td>
				#end
				<td></td>
			</tr>
			<tr>
				#if($bulletin.fields.location != "")
					<td>Location:</td>
					<td>$bulletin.fields.location</td>
				#end
				<td></td>
			</tr>
			<tr>
				#if($bulletin.fields.language != "")
					<td>Language:</td>
					<td>$bulletin.fields.language</td>
				#end
				<td></td>
			</tr>
		</table>
		
		<br/><br/>

		#parse("PreviousNextBulletin.vm")
		#parse ("ResultFooter.vm")
    </body>
</html>
		