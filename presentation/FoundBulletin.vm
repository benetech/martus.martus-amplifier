<html>
    <head>
    	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    	<title>Martus Amplifier Search Query</title>
    </head>
    
    <body>
		#include ("ResultHeader.vm")
		
		<p><font size='-1'><a href='some url'>Printer Friendly Version</a></font></p>
		<table>
			<tr>
				<td><font size="+1">Title:</font></td>
				<td><font size="+1"><strong>$bulletin.fields.title</strong></font></td>
			
			</tr>
			<tr>
				<td>Location:</td>
				<td>$bulletin.fields.location</td>
				<td></td>
			</tr>
			<tr>
				<td>Event Date:</td>
				<td>$bulletin.fields.eventdate</td>
				<td></td>
			</tr>
			<tr>
				<td>Author:</td>
				<td>$bulletin.fields.author</td>
				<td></td>
			</tr>
			<tr>
				<td>Date Submitted:</td>
				<td>$bulletin.fields.entrydate</td>
				<td></td>
			</tr>
			<tr>
				<td>Summary:</td>
				<td>$bulletin.fields.summary</td>
				<td></td>
			</tr>
			<tr>
				<td>Details:</td>
				<td>$bulletin.fields.publicinfo</td>
				<td></td>
			</tr>
			<tr>
				<td>Keywords:</td>
				<td>$bulletin.fields.keywords</td>
				<td></td>
			</tr>
		</table>
		
		<br/><br/><br/>

		<center>
		#if ($previousBulletin >= 0)
			<a href='FoundBulletin?index=$previousBulletin'>&lt;&lt; Previous</a>
		#end
			&nbsp;&nbsp;&nbsp;
		#if ($nextBulletin >= 0)
			<a href='FoundBulletin?index=$nextBulletin'>Next &gt;&gt;</a>
		#end
		</center>
		#include ("ResultFooter.vm")
    </body>
</html>
		